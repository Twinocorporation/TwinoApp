<?xml version="1.0" encoding="UTF-8"?>
<Context antiJARLocking="true" path="/TwinoServer">
	<Resource
    	auth="Container"
    	type="javax.sql.DataSource"
    	driverClassName="org.apache.derby.jdbc.ClientDriver"
    	url="jdbc:derby://localhost:1527/Twino;create=true"
    	maxActive="10"
    	maxIdle="60"
    	maxWait="10000"
    	name="jdbc/TwinoServer"
    	username="Twino"
    	password="twino"
	/>
</Context>

